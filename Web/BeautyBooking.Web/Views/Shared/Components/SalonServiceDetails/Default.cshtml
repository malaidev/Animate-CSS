@model BeautyBooking.Web.ViewModels.SalonServices.SalonServiceDetailsViewModel
@{
    ViewData["Title"] = "Default";
}

<div class="form-group row">
    <label class="col-sm-2 col-form-label">Salon</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.SalonName</div>
    </div>
</div>
<div class="form-group row">
    <label class="col-sm-2 col-form-label">Address</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.SalonCityName, @Model.SalonAddress</div>
    </div>
</div>
<div class="form-group row">
    <label class="col-sm-2 col-form-label">Service</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.ServiceName</div>
    </div>
</div>
